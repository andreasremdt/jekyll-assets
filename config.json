{
  "sass": {
    "src": "assets/sass/test.scss",
    "output": {
      "dir": "css",
      "name": "styles.min.css"
    },
    "options": {
      "outputStyle": "compressed"
    },
    "sourcemap": true,
    "prefix": true,
    "browsers": ["last 5 versions"]
  },
  "js": {
    "src": "assets/js",
    "output": {
      "dir": "js",
      "name": "app.js"
    },
    "minify": true,
    "concat": true,
    "babel": true,
    "sourcemap": true
  },
  "images": {
    "inputDir": "assets/images",
    "outputDir": "images",
    "options": {
      "webp": {
        "quality": 75,
        "preset": "default"
      },
      "others": {}
    }
  },
  "jekyll": {
    "deployDir": "_site",
    "baseDir": "jekyll",
    "args": ["--watch", "--incremental", "--drafts"],
    "port": 4000
  }
}